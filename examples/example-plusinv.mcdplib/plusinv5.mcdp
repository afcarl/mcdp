mcdp {
	f = instance mcdp  {
	    provides a [Nat]
	    
	    requires x [Nat]
	    requires y [Nat]
	    
	    x + y >= a
	}

	provides z [Nat]

	f.a >= sqrt(f.x) + sqrt(f.y) + z 

	requires x, y for f
}


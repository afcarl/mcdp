T = 10 s	
	
explicit {
	requires energy [J]
	requires time [J]
	
	implementation P [W]
	
	1 W <= P <= 10 W
	
	
	alpha := 1 W
	
	energy == P * T
	time == 1 second * exp(- P / alpha)  
}

battery := explicit {

	V_cell := 1.0 V
	W_cell := 100 g
	
	specific_energy := 10 J / g
	
	implementation N cells
	
	1 <= N <= 10
	
	weight == N * W_cell
	capacity == weight * cell 
}







  
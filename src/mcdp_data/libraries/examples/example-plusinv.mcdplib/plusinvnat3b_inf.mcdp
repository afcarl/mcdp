# note that this one does not have a finite solution
# It is equivalent to
#   x + y >= x^2 + y^2 + 10
mcdp {
    f = instance mcdp  {
        provides a [Nat]
        
        requires x [Nat]
        requires y [Nat]
        
        x + y >= a
    }

    f.a >= square(f.x) + square(f.y) + Nat:10

    requires x, y for f
}
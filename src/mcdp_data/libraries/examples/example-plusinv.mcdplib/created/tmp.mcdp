flatten mcdp {
    f = instance mcdp {
        requires x [Nat]
        requires y [Nat]
        provides z [Nat] 
        x + y >= z
    }
    
    requires x, y for f
    #provides v [Nat]
    
    f.z >= sqrt(f.x) + sqrt(f.y) + nat:4
    
}
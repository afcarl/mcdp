# This converts from TypeC to TypeL
mcdp {
    provides out [`power]
    requires in  [`power] 
    requires cost [USD]

    # This device costs $5 
    cost >= 5 USD

    take(in,  0) >= `socket_type : TypeL
    take(out, 0) <= `socket_type : TypeC 
 
    take(in, 1) >=  take(out, 1) # voltages
    take(in, 2) >=  take(out, 2) # amperes
}
mcdp {
   provides motion [`Motion]
   provides payload [`Payload] 

   requires budget [USD]

   requires pwm1 [`PWM]
   requires pwm2 [`PWM]

   mass = (provided payload).mass
   shape = (provided payload).shape
 
  # limit x and y
   take(shape, 0) <= 10cm
   take(shape, 1) <= 11cm

   (provided motion).velocity <= 35 cm/ s
   current = mass * c
    
}

